{
  "name": "@cucumber/suggest",
  "version": "0.0.2",
  "description": "Library for suggesting steps",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cucumber",
      "aliases": ["Cucumber", "cucumber"],
      "extensions": [".feature"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cucumber",
      "scopeName": "source.gherkin",
      "path": "./syntaxes/cucumber.tmLanguage.json"
    }]
  },
  "scripts": {
    "test": "npm run cucumber && npm run mocha",
    "mocha": "mocha",
    "cucumber": "cucumber-js",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc --build tsconfig.build.json",
    "watch": "tsc --build tsconfig.build.json --watch",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cucumber/suggest-javascript.git"
  },
  "keywords": [
    "cucumber",
    "steps",
    "regexp",
    "regex"
  ],
  "author": "Cucumber Limited <cukes@googlegroups.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cucumber/suggest-javascript/issues"
  },
  "homepage": "https://github.com/cucumber/suggest-javascript#readme",
  "devDependencies": {
    "@cucumber/cucumber": "7.3.1",
    "@types/js-search": "1.4.0",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.7",
    "fuse.js": "6.4.6",
    "js-search": "2.0.0",
    "mocha": "9.0.3",
    "txtgen": "2.2.8",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@cucumber/cucumber-expressions": "12.1.1",
    "@cucumber/gherkin": "20.0.1",
    "@cucumber/gherkin-utils": "6.0.0",
    "@cucumber/messages": "17.0.1",
    "@cucumber/message-streams": "3.0.0",
    "@types/vscode": "^1.52.0",
    "vscode-languageserver": "7.0.0",
    "vscode-languageserver-textdocument": "1.0.1"
  }
}
